//Remove duplicates in place

#include <stdio.h>
int remove_duplicates(int *a, int n)
{
  int insert_idx = 1;
  for(int i=1;i<n;i++)
  {
    if(a[i] != a[i-1])
    {
      a[insert_idx] = a[i];
      insert_idx++; //Move insert index only if they are not equal
    }
  }
  return insert_idx;
}

int main(void){

  int a[10] = {1, 2, 2, 4, 5, 6, 6, 7, 8, 9};
  int valid_entries = remove_duplicates(a, sizeof(a)/sizeof(int));

  for(int i=0;i<10;i++)
  {
    printf("Index[%d]: %d \n", i,a[i]);
  }
}
